    "license": "MIT",
    "authors": [
        {
            "name": "Magnus Nordlander",
            "email": "magnus@fervo.se",
            "homepage": "http://www.fervo.se/",
            "role": "Developer"
        },
        {
            "name": "Tobias Nyholm",
            "email": "tobias@happyr.com",
